{
    "compilerOptions": {
	"target": "es6",
        "lib": ["dom", "es2015"],
        "alwaysStrict": true,
        "strictBindCallApply": true,
        "strictFunctionTypes": true,
        "noImplicitAny": true,
        "noFallthroughCasesInSwitch": true,
        "noUnusedLocals": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "removeComments": true,
        "outFile": "javascript/regex_language.js",
        "noEmitOnError": true
    },
    "files": [
        "src/regex_language.ts"
    ]
}
